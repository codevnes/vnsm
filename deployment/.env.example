# Domain settings
DOMAIN=example.com
ACME_EMAIL=your-email@example.com

# Traefik dashboard authentication (generate with: htpasswd -nb admin secure_password)
TRAEFIK_USER=admin
TRAEFIK_PASSWORD=$$apr1$$xxxxxxxxxxxxxxxxxxxxxx

# Adminer authentication (generate with: htpasswd -nb admin secure_password)
ADMINER_USER=admin
ADMINER_PASSWORD=$$apr1$$xxxxxxxxxxxxxxxxxxxxxx

# Database settings
DB_USER=vnsm_user
DB_PASSWORD=change_this_password
DB_NAME=vnsm

# JWT for authentication
JWT_SECRET=change_this_secret_long_random_string

# Backup settings
BACKUP_KEEP_DAYS=7
BACKUP_PREFIX=vnsm 